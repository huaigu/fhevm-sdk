{
  "name": "@0xbojack/fhevm-vue",
  "version": "0.2.0",
  "description": "Vue 3 composables for FHEVM SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "vue",
    "vue3",
    "composables",
    "privacy",
    "zama",
    "blockchain"
  ],
  "author": "Zama",
  "license": "BSD-3-Clause-Clear",
  "repository": {
    "type": "git",
    "url": "https://github.com/zama-ai/fhevm-sdk.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/zama-ai/fhevm-sdk#readme",
  "bugs": {
    "url": "https://github.com/zama-ai/fhevm-sdk/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@0xbojack/fhevm-core": "^0.2.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "ethers": "^6.13.7"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.6",
    "@types/node": "^22.7.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9",
    "vue": "^3.5.13"
  }
}
